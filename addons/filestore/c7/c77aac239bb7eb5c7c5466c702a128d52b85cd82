rounding){var decimals=this.pos.dp['Product Unit of Measure'];var rounding=Math.max(unit.rounding,Math.pow(10,-decimals));this.quantity=round_pr(quant,rounding);this.quantityStr=field_utils.format.float(this.quantity,{digits:[69,decimals]});}else{this.quantity=round_pr(quant,1);this.quantityStr=this.quantity.toFixed(0);}}else{this.quantity=quant;this.quantityStr=''+this.quantity;}}
